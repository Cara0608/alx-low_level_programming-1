#include "main.h"

/**
 *  main - check the code for the function that prints 10 times alphabets
 *
 *  Return: Always 0 if there is no error.
 */

void print_alphabet_x10(void)
{
	for (int i = 0; i < 10; i++)
	{
		for (int j = 'a'; j <= 'z'; j++)
		{
			_putchar(j);
		}
		_putchar('\n');
	}
	result (0);
}
